{
  "name": "book-a-meal",
  "version": "1.0.0",
  "engines": {
    "node": "10.1.0",
    "npm": "6.1.0"
  },
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -r esm --config webpack.dev.js --open",
    "build:client": "webpack -r esm --config webpack.prod.js",
    "build:clientp": "cross-env NODE_ENV=production npm run build:client",
    "start:dev": "cross-env NODE_ENV=development nodemon --watch server -r dotenv/config -r esm server/src/app.js",
    "start:prod": "cross-env NODE_ENV=production nodemon --watch server -r dotenv/config -r esm server/src/app.js",
    "start:babel": "nodemon --watch server -r dotenv/config --exec babel-node server/src/app.js",
    "build:server": "babel server/src -d server/lib --copy-files",
    "server:setup": "sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production",
    "build": "npm-run-all build:client build:server",
    "heroku-postbuild": "npm run server:setup && npm run build",
    "herokup": "npm run build:clientp && cross-env DATABASE_URL=postgres://zzwwvxmv:r9blS4h6NG-jCJ8VdYY_gEFwljCXFtos@elmer.db.elephantsql.com:5432/zzwwvxmv sequelize db:migrate --env=production && npm run start:prod",
    "pretest:server": "npm run refresh:test",
    "test:server": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha server/test --t 5000 --exit --recursive -r dotenv/config -r esm",
    "test:babel": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha server/test --exit --recursive -r dotenv/config -r babel-polyfill -r babel-register",
    "test:client": "jest --env=jsdom --coverage",
    "test:client:watch": "jest --env=jsdom --watch",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report --reporter=text-lcov",
    "lint": "eslint server/src/**/*.js --fix",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env=test",
    "drop": "sequelize db:migrate:undo:all",
    "drop:test": "sequelize db:migrate:undo:all --env=test",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env=test",
    "refresh": "npm-run-all drop migrate seed",
    "refresh:test": "npm-run-all drop:test migrate:test seed:test"
  },
  "repository": {
