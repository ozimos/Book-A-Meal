{
  "name": "book-a-meal",
  "version": "1.0.0",
  "engines": {
    "node": "10.1.0"
  },
  "description": "Book-A-Meal is an application that allows customers to make food orders and helps the food",
  "main": "index.js",
  "scripts": {
    "start": "webpack -r esm --mode development",
    "build": "webpack -r esm --mode",
    "start:dev": "nodemon --watch server -r dotenv/config -r esm server/src/bin/www.js",
    "start:babel": "nodemon --watch server -r dotenv/config --exec babel-node server/src/app.js",
    "heroku-postbuild": "npm run build && sequelize db:migrate:undo:all --env=production && sequelize db:migrate --env=production",
    "pretest": "npm run refresh:test",
    "test": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha server/test --t 5000 --exit --recursive -r dotenv/config -r esm",
    "test:babel": "cross-env NODE_ENV=test TOKEN_PASSWORD=test nyc mocha server/test --exit --recursive -r dotenv/config -r babel-polyfill -r babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report": "nyc report --reporter=text-lcov",
    "lint": "eslint server/src/**/*.js --fix",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env=test",
    "drop": "sequelize db:migrate:undo:all",
    "drop:test": "sequelize db:migrate:undo:all --env=test",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env=test",
    "refresh": "npm run drop && npm run migrate && npm run seed",
    "refresh:test": "npm run drop:test && npm run migrate:test && npm run seed:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozimos/Book-A-Meal.git"
  },
  "author": "Tovieye Moses Ozi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ozimos/Book-A-Meal/issues"
  },
  "homepage": "https://github.com/ozimos/Book-A-Meal#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "eslint": "^4.9.0",
    "eslint-plugin-import": "^2.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.1.1",
    "mockdate": "^2.0.2",
    "nodemon": "^1.17.3",
    "nyc": "^11.7.1",
    "sequelize-cli": "^4.0.0",
    "testdouble": "^3.7.0",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "date-fns": "^1.29.0",
    "dotenv": "^5.0.1",
    "esm": "^3.0.24",
    "express": "^4.16.3",
    "express-joi-validation": "^0.2.1",
    "joi": "^13.2.0",
    "jsonwebtoken": "^8.2.1",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.7",
    "swagger-ui-express": "^3.0.9",
    "yamljs": "^0.3.0"
  }
}
